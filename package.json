{
  "name": "ci2group-staticboiler",
  "version": "1.0.0",
  "description": "Static Website boiler for CDG projects",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch _src/sass:_site/css",
    "build:sass": "sass _src/sass:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "http://ci2dev01/Ci2Development/Ci2Group-staticBoiler/_git/Ci2Group-staticBoiler"
  },
  "author": "Ci2 Group",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1"
  },
  "dependencies": {
    "clean-css": "^5.1.2",
    "eleventy-plugin-purgecss": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.8"
  }
}
